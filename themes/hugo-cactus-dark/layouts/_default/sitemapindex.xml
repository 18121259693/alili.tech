<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
  xmlns:xhtml="http://www.w3.org/1999/xhtml">
 {{ range .Data.Pages }}
  <url>
    <loc>{{ .Permalink }}</loc>
    <lastmod>{{ safeHTML ( .Lastmod.Format "2006-01-02T15:04:05-07:00" ) }}</lastmod>
    <data>
        <display>
        <title>{{.Title}}</title>
        <pubTime>{{ safeHTML ( .Lastmod.Format "2006-01-02T15:04:05-07:00" ) }}</pubTime>
              <breadCrumb title="reprint" url="{{ .Permalink }}"/>
        </display>
    </data>
    </url>
     {{ end }}
</urlset>